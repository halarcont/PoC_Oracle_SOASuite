          <condition>$vError = false()</condition>
          <assign name="Assign_OutboundOK">
            <copy>
              <from>$vUserOFSC</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$Invoke_Outbound_set_message_status_InputVariable.parameters/user/login</to>
            </copy>
            <copy>
              <from>$inputVariable.body/ns8:company_msg</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$Invoke_Outbound_set_message_status_InputVariable.parameters/user/company</to>
            </copy>
            <copy>
              <from>$Invoke_CommonUtility_getAuth_OutputVariable.reply/ns7:data/ns7:now</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$Invoke_Outbound_set_message_status_InputVariable.parameters/user/now</to>
            </copy>
            <copy>
              <from>$Invoke_CommonUtility_getAuth_OutputVariable.reply/ns7:data/ns7:auth</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$Invoke_Outbound_set_message_status_InputVariable.parameters/user/auth_string</to>
            </copy>
            <copy>
              <from>$inputVariable.body/ns8:message_id</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$Invoke_Outbound_set_message_status_InputVariable.parameters/messages/message/message_id</to>
            </copy>
            <copy>
              <from>"delivered"</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$Invoke_Outbound_set_message_status_InputVariable.parameters/messages/message/status</to>
            </copy>
            <copy>
              <from>concat('Proceso de instalación de inventario ejecutado exitosamente. FlowId:',ora:getFlowId())</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$Invoke_Outbound_set_message_status_InputVariable.parameters/messages/message/description</to>
            </copy>
            <copy>
              <from>'#params?xi_activation_error=&amp;xi_activation_status=Inventario Desinstalado&amp;xa_substatus=validated&amp;xi_reason_remove='</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$Invoke_Outbound_set_message_status_InputVariable.parameters/messages/message/data</to>
            </copy>
          </assign>
        </if>
        <invoke name="Invoke_Outbound" bpelx:invokeAsDetail="no" partnerLink="OutboundOFSC" portType="ns6:ToaPortType"
                operation="set_message_status" inputVariable="Invoke_Outbound_set_message_status_InputVariable"
                outputVariable="Invoke_Outbound_set_message_status_OutputVariable"
                xmlns:bpelx="http://schemas.oracle.com/bpel/extension"/>
      </sequence>
    </if>
  </sequence>
</process>