  <faultHandlers>
    <catchAll>
      <if name="If_GetAuth">
        <condition>$Invoke_CommonUtility_getAuth_OutputVariable.reply/ns7:result/ns7:code = 0</condition>
        <sequence name="Sequence1">
          <assign name="Assign_Outbound">
            <copy>
              <from>$vUserOFSC</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$Invoke_Outbound_set_message_status_InputVariable.parameters/user/login</to>
            </copy>
            <copy>
              <from>$inputVariable.body/ns8:company_msg</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$Invoke_Outbound_set_message_status_InputVariable.parameters/user/company</to>
            </copy>
            <copy>
              <from>$Invoke_CommonUtility_getAuth_OutputVariable.reply/ns7:data/ns7:now</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$Invoke_Outbound_set_message_status_InputVariable.parameters/user/now</to>
