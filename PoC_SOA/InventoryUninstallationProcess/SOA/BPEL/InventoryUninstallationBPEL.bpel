    <partnerLink name="CommonUtility" partnerLinkType="ns2:CommonUtility" partnerRole="CommonUtilityProvider"/>
    <partnerLink name="DB_ORGANIZATION" partnerLinkType="ns3:DB_ORGANIZATION_plt" partnerRole="DB_ORGANIZATION_role"/>
