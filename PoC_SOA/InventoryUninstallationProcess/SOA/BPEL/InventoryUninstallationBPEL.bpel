  <sequence name="main">
    <!-- Receive input from requestor. (Note: This maps to operation defined in InventoryUninstallationBPEL.wsdl) -->
    <receive name="receiveInput" partnerLink="inventoryuninstallationbpel_client"
             portType="ns1:Consume_Message_InvUninst_ptt" operation="Consume_Message_InvUninst" variable="inputVariable"
             createInstance="yes"/>
    <assign name="Assign_CommonUtility">
      <copy>
        <from>"client_rest_api"</from>
        <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$vUserOFSC</to>
      </copy>
      <copy>
        <from>$vUserOFSC</from>
        <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$Invoke_CommonUtility_getAuth_InputVariable.request/ns7:user</to>
      </copy>
      <copy>
        <from>ora:getPreference('clientSecret')</from>
        <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$Invoke_CommonUtility_getAuth_InputVariable.request/ns7:password</to>
      </copy>
    </assign>
    <assign name="AssignInicialize">
      <copy>
        <from>0</from>
        <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$NumRetryUninstallation</to>
      </copy>
      <copy>
        <from><literal>GENERAL_ERROR</literal></from>
        <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varCodeIntegration</to>
      </copy>
      <copy>
        <from>concat('CREATE_SUBINVENTORY_ ',$inputVariable.body/ns8:integration_type)</from>
        <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varNameIntegration</to>
      </copy>
    </assign>
    <scope name="ScopeValidacion" xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable">
      <faultHandlers>
        <catch faultName="bpelx:bindingFault" faultVariable="varBindingFault"
               faultMessageType="bpelx:RuntimeFaultMessage">
          <sequence name="Sequence16">
            <assign name="AssignContentMail">
              <copy>
                <from>concat('Error en la creación de SubInvetario:  ' ,$inputVariable.body/ns8:xa_plan_code, ', para numero de orden: ',$inputVariable.body/ns8:appt_number)</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varMessageMail</to>
              </copy>
            </assign>
            <assign name="AssignInputMailJMS">
              <copy>
                <from><literal>-1</literal></from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$InvokeJSMCompletedProcess_Produce_Message_InputVariable.body/ns17:NotificationHeader/ns17:Type</to>
              </copy>
              <copy>
                <from>$varBindingFault.code</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$InvokeJSMCompletedProcess_Produce_Message_InputVariable.body/ns17:NotificationHeader/ns17:ExecutionDetail/ns17:CodeError</to>
              </copy>
              <copy>
                <from>$varBindingFault.summary</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$InvokeJSMCompletedProcess_Produce_Message_InputVariable.body/ns17:NotificationHeader/ns17:ExecutionDetail/ns17:DescriptionError</to>
              </copy>
              <copy>
                <from>$varCodeIntegration</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$InvokeJSMCompletedProcess_Produce_Message_InputVariable.body/ns17:NotificationHeader/ns17:IntegrationCode</to>
              </copy>
              <copy>
                <from>$varNameIntegration</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$InvokeJSMCompletedProcess_Produce_Message_InputVariable.body/ns17:NotificationHeader/ns17:IntegrationName</to>
              </copy>
              <copy>
                <from>$varMessageMail</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$InvokeJSMCompletedProcess_Produce_Message_InputVariable.body/ns17:NotificationHeader/ns17:ContentEmail</to>
              </copy>
            </assign>
            <invoke name="InvokeJSMCompletedProcess" partnerLink="ProduceJMSNotification"
                    portType="ns16:Produce_Message_ptt" operation="Produce_Message"
                    inputVariable="InvokeJSMCompletedProcess_Produce_Message_InputVariable" bpelx:invokeAsDetail="no"/>
          </sequence>
        </catch>
        <catch faultName="bpelx:remoteFault" faultVariable="varRemoteFault"
               faultMessageType="bpelx:RuntimeFaultMessage"><sequence name="Sequence17"><assign name="AssignContentMail"
                                                                                                xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable">
              <copy>
                <from>concat('Error en la creación de SubInvetario:  ' ,$inputVariable.body/ns8:xa_plan_code, ', para numero de orden: ',$inputVariable.body/ns8:appt_number)</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varMessageMail</to>
              </copy>
            </assign><assign name="AssignInputMailJMS"
                                                                                                                                                                                                                                                            xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable">
              <copy>
                <from><literal>-1</literal></from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$InvokeJSMCompletedProcess_Produce_Message_InputVariable.body/ns17:NotificationHeader/ns17:Type</to>
              </copy>
              <copy>
                <from>$varRemoteFault.code</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$InvokeJSMCompletedProcess_Produce_Message_InputVariable.body/ns17:NotificationHeader/ns17:ExecutionDetail/ns17:CodeError</to>
              </copy>
              <copy>
                <from>$varRemoteFault.summary</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$InvokeJSMCompletedProcess_Produce_Message_InputVariable.body/ns17:NotificationHeader/ns17:ExecutionDetail/ns17:DescriptionError</to>
              </copy>
              <copy>
                <from>$varCodeIntegration</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$InvokeJSMCompletedProcess_Produce_Message_InputVariable.body/ns17:NotificationHeader/ns17:IntegrationCode</to>
              </copy>
              <copy>
                <from>$varNameIntegration</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$InvokeJSMCompletedProcess_Produce_Message_InputVariable.body/ns17:NotificationHeader/ns17:IntegrationName</to>
              </copy>
              <copy>
                <from>$varMessageMail</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$InvokeJSMCompletedProcess_Produce_Message_InputVariable.body/ns17:NotificationHeader/ns17:ContentEmail</to>
              </copy>
            </assign><invoke name="InvokeJSMCompletedProcess"
                                                                                                                                                                                                                                                            partnerLink="ProduceJMSNotification"
                                                                                                                                                                                                                                                            portType="ns16:Produce_Message_ptt"
                                                                                                                                                                                                                                                            operation="Produce_Message"
                                                                                                                                                                                                                                                            inputVariable="InvokeJSMCompletedProcess_Produce_Message_InputVariable"
                                                                                                                                                                                                                                                            bpelx:invokeAsDetail="no"
                                                                                                                                                                                                                                                            xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
                                                                                                                                                                                                                                                            xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"/></sequence></catch>
        <catchAll><sequence name="Sequence18"><assign name="AssignContentMail"
                                                      xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable">
              <copy>
                <from>concat('Error en la creación de SubInvetario:  ' ,$inputVariable.body/ns8:xa_plan_code, ', para numero de orden: ',$inputVariable.body/ns8:appt_number)</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varMessageMail</to>
              </copy>
            </assign><assign name="AssignInputMailJMS"
                                                                                                                                                                                                                  xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable">
              <copy>
                <from><literal>-1</literal></from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$InvokeJSMCompletedProcess_Produce_Message_InputVariable.body/ns17:NotificationHeader/ns17:Type</to>
              </copy>
              <copy>
                <from>$varCodeIntegration</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$InvokeJSMCompletedProcess_Produce_Message_InputVariable.body/ns17:NotificationHeader/ns17:IntegrationCode</to>
              </copy>
              <copy>
                <from>$varNameIntegration</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$InvokeJSMCompletedProcess_Produce_Message_InputVariable.body/ns17:NotificationHeader/ns17:IntegrationName</to>
              </copy>
              <copy>
                <from>$varMessageMail</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$InvokeJSMCompletedProcess_Produce_Message_InputVariable.body/ns17:NotificationHeader/ns17:ContentEmail</to>
              </copy>
              <copy>
                <from><literal>-1</literal></from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$InvokeJSMCompletedProcess_Produce_Message_InputVariable.body/ns17:NotificationHeader/ns17:ExecutionDetail/ns17:CodeError</to>
              </copy>
            </assign><invoke name="InvokeJSMCompletedProcess"
                                                                                                                                                                                                                  partnerLink="ProduceJMSNotification"
                                                                                                                                                                                                                  portType="ns16:Produce_Message_ptt"
                                                                                                                                                                                                                  operation="Produce_Message"
                                                                                                                                                                                                                  inputVariable="InvokeJSMCompletedProcess_Produce_Message_InputVariable"
                                                                                                                                                                                                                  bpelx:invokeAsDetail="no"
                                                                                                                                                                                                                  xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
                                                                                                                                                                                                                  xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"/></sequence></catchAll>
      </faultHandlers>
      <sequence name="Sequence12">
        <scope name="Scope_Consulta_CreacionSubInventario">
          <faultHandlers>
            <catchAll>
              <if name="If_Catch_Process">
                <condition>$NumRetryUninstallation &lt;3</condition>
                <sequence name="Sequence15">
                  <assign name="Assign_Num_Process">
                    <copy>
                      <from>$NumRetryUninstallation +1</from>
                      <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$NumRetryUninstallation</to>
                    </copy>
                  </assign>
                  <wait name="Wait_Process">
                    <for>'PT30S'</for>
                  </wait>
                  <extensionActivity>
                    <bpelx:replay name="Replay_Process_Consulta_Creación" scope="Scope_Consulta_CreacionSubInventario"/>
                  </extensionActivity>
                </sequence>
                <else>
                  <rethrow name="Rethrow_Process"/>
                </else>
              </if>
            </catchAll>
          </faultHandlers>
          <sequence>
            <assign name="Assign_Consulta" xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable">
              <copy>
                <from>concat('OrganizationCode=PTN_CLIE;SecondaryInventoryName=',$inputVariable.body/ns8:xa_plan_code)</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$InvokeConsultaSub_ConsultarSub_InputVariable.request/ns13:q</to>
              </copy>
            </assign>
            <invoke name="InvokeConsultaSub" partnerLink="SubInventorySCM" portType="ns11:SubInventorySCM_ptt"
                    operation="ConsultarSub" inputVariable="InvokeConsultaSub_ConsultarSub_InputVariable"
                    outputVariable="InvokeConsultaSub_ConsultarSub_OutputVariable"
                    xmlns:bpelx="http://schemas.oracle.com/bpel/extension" bpelx:invokeAsDetail="no">
              <bpelx:toProperties>
                <bpelx:toProperty name="rest.binding.http.Content-Type">"application/vnd.oracle.adf.resourceitem+json"</bpelx:toProperty>
              </bpelx:toProperties>
