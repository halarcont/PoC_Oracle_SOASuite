            </copy>
            <copy>
              <from>"failed"</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$Invoke_Outbound_set_message_status_InputVariable.parameters/messages/message/status</to>
            </copy>
            <copy>
              <from>concat('Ocurrio un error en el proceso de desinstalación de inventario. FlowId:',ora:getFlowId())</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$Invoke_Outbound_set_message_status_InputVariable.parameters/messages/message/description</to>
            </copy>
            <copy>
              <from>'#params?xi_activation_error=Ocurrio un error en el proceso de desinstalación del inventario&amp;xi_activation_status=&amp;xa_substatus=error_provisioning'</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$Invoke_Outbound_set_message_status_InputVariable.parameters/messages/message/data</to>
            </copy>
          </assign>
          <invoke name="Invoke_Outbound" bpelx:invokeAsDetail="no" partnerLink="OutboundOFSC" portType="ns6:ToaPortType"
